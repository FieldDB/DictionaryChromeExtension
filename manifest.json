{
  "name": "Google Dictionary Lookup",
  "version": "2.1.5",
  "description": "Look up any word in Google Dictionary without leaving the page.",
  "icons": { "128": "icon.png" },
  "options_page": "options.htm",
  "background_page": "background.htm",
  "content_scripts": [{
    "matches": [ "file://*/*", "http://*/*", "https://*/*" ],
    "run_at": "document_end",
    "css": [ "frame.css" ],
    "js": [ "lookup.js" ]
  }],
  "permissions": [
    "tabs",
    "http://www.google.com/",
    "http://ajax.googleapis.com/"
  ]
}