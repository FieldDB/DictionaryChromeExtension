{
  "name": "Dictionary Lookup",
  "version": "3.0.1",
  "description": "Look up any word with a simple click and without leaving the page.",
  "icons": { "128": "icon.png" },
  "options_page": "options.htm",
  "background_page": "background.htm",
  "content_scripts": [{
    "matches": [ "http://*/*", "https://*/*" ],
    "run_at": "document_end",
    "css": [ "frame.css" ],
    "js": [ "lookup.js" ]
  }],
  "permissions": [
    "tabs",
    "http://www.onelook.com/"
  ]
}